{
  "name": "OSUN Dark Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0F1419",
    "editor.foreground": "#DADADA"
  },
  "tokenColors": [
    {
      "scope": ["keyword.control.osun"],
      "settings": { "foreground": "#FF9E64", "fontStyle": "bold" }
    },
    {
      "scope": ["string.quoted.double.osun"],
      "settings": { "foreground": "#9ECE6A" }
    },
    {
      "scope": ["variable.other.osun"],
      "settings": { "foreground": "#7AA2F7" }
    },
    {
      "scope": ["constant.numeric.osun"],
      "settings": { "foreground": "#F7768E" }
    },
    {
      "scope": ["comment.line.double-slash.osun"],
      "settings": { "foreground": "#565F89", "fontStyle": "italic" }
    }
  ]
}
